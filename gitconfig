[alias]
	br = branch
	ci = commit
	co = checkout
	fp = fetch --prune --all
	graph = log --graph --decorate --oneline --all
	st = status -u -sb
	save = stash -u
[color]
	ui = auto
[core]
	editor = vim
[push]
	default = matching
[rerere]
	enabled = true
[rebase]
	autosquash = true
[branch]
  autosetuprebase = always

